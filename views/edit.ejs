<<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Meu Crud em nodejs</title>
  </head>
  <body>

    <tbody>
      <h2>Editar</h2>
      <% data.forEach(function(details) { %>
          <form action="/edit/<%= details._id %>" method="POST">
          <input type="text" value="<%= details.veiculo %>" name="veiculo" >
          <input type="text" value="<%= details.cor %>" name="cor" >
          <br />
          <button><<a href="/show">Todos</a></button>
          <button type="submit">Editar</button>

        </form>
        <% }) %>

    </tbody>

  </body>
</html>
